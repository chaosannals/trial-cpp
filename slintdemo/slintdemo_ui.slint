export component HelloWorld inherits Window {
    width: 400px;
    height: 400px;

    // 属性 生成 get_my_label 和 set_my_label 函数，in out 分别用来开放这2个行数public 
    in-out property my_label <=> label.text;

    label := Text {
       y: parent.width / 2;
       x: parent.x + 200px;
       text: "Hello, world";
       color: blue;
    }
}